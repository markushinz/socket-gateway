{
    "name": "socket-gateway",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "lint": "eslint . --ext .ts,.tsx",
        "test": "jest --collectCoverage --detectOpenHandles --silent=true",
        "build": "rm -rf ./dist && tsc",
        "start": "node dist/${SG_MODE:-\"outer-layer\"}/server.js",
        "serve": "npm run build && SG_TARGETS='{\"targets\":{\"localhost\":{\"hostname\":\"jsonplaceholder.typicode.com\"}}}' node dist/server.js"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "js-yaml": "^3.14.0",
        "socket.io": "^3.0.3",
        "socket.io-client": "^3.0.3",
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.9",
        "@types/js-yaml": "^3.12.5",
        "@types/node-forge": "^0.9.5",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.14.0",
        "jest": "^26.6.3",
        "node-forge": "^0.10.0",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.2"
    }
}
